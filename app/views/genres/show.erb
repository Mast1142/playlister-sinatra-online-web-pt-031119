<h1>Genre Page</h1>
<h2><%=@genre.name%></h2>


<label>Songs:</Label>
  <ul>
    <%@genre.songs.each do |song|%>
     <a href="/songs/<%=song.slug%>"><%= song.name %></a><br/>
    <%end%>
  </ul>

<label>Artists:</Label>
  <ul>
    <%@genre.artists.each do |artist|%>
     <a href="/artists/<%=artist.slug%>"><%= artist.name %></a><br/>
    <%end%>
  </ul>
